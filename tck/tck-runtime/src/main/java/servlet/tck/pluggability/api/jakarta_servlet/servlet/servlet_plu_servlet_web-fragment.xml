<web-fragment xmlns="https://jakarta.ee/xml/ns/jakartaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee
              https://jakarta.ee/xml/ns/jakartaee/web-fragment_5_0.xsd"
              version="5.0">
    <servlet>
        <servlet-name>DoServletInfoTestLogicalName</servlet-name>
        <servlet-class>servlet.tck.api.jakarta_servlet.servlet.DoServletInfoTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoDestroyedTestLogicalName</servlet-name>
        <servlet-class>servlet.tck.api.jakarta_servlet.servlet.DoDestroyedTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoInit2TestLogicalName</servlet-name>
        <servlet-class>servlet.tck.api.jakarta_servlet.servlet.DoInit2TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoServletConfigTestLogicalName</servlet-name>
        <servlet-class>servlet.tck.api.jakarta_servlet.servlet.DoServletConfigTestServlet</servlet-class>
        <init-param>
            <param-name>Language</param-name>
            <param-value>Java</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>PUTestLogicalName</servlet-name>
        <servlet-class>servlet.tck.api.jakarta_servlet.servlet.PUTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoServiceTestLogicalName</servlet-name>
        <servlet-class>servlet.tck.api.jakarta_servlet.servlet.DoServiceTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoInit1TestLogicalName</servlet-name>
        <servlet-class>servlet.tck.api.jakarta_servlet.servlet.DoInit1TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DoServletInfoTestLogicalName</servlet-name>
        <url-pattern>/DoServletInfoTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoDestroyedTestLogicalName</servlet-name>
        <url-pattern>/DoDestroyedTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoInit2TestLogicalName</servlet-name>
        <url-pattern>/DoInit2Test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoServletConfigTestLogicalName</servlet-name>
        <url-pattern>/DoServletConfigTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PUTestLogicalName</servlet-name>
        <url-pattern>/PUTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoServiceTestLogicalName</servlet-name>
        <url-pattern>/DoServiceTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoInit1TestLogicalName</servlet-name>
        <url-pattern>/DoInit1Test</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>54</session-timeout>
    </session-config>
</web-fragment>
