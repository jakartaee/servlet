    </filter>
    <filter>
        <filter-name>DuplicateFilterString</filter-name>
        <filter-class>com.sun.ts.tests.servlet.api.jakarta_servlet.servletcontext30.DuplicateFilterString</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>DuplicateFilterClass</filter-name>
        <url-pattern>/FilterTestServlet</url-pattern>
     </filter-mapping>
    <filter-mapping>
        <filter-name>DuplicateFilterString</filter-name>
